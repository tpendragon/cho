<%= form_with(model: test_model, local: true, enctype:"multipart/form-data") do |form| %>
  <!--<input name="test_model[file]" type="hidden" value="<%= @test_model.cached_files_data %>">-->
  <!--<%= form.hidden_field :files, value: @test_model.cached_files_data %>-->
  <%= form.file_field :files , webkitdirectory: "webkitdirectory", mozdirectory: "mozdirectory", msdirectory: "msdirectory", odirectory: "odirectory", directory: "directory", multiple: "multiple", name: 'test_model[files][]' %>

  <% if test_model.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(test_model.errors.count, "error") %> prohibited this test_model from being saved:</h2>

      <ul>
      <% test_model.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
